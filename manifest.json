{
    "manifest_version": 2,
    "name": "Task Manager",
    "description": "This extension will show Task Manager",
    "version": "1.0",
    "background_page": "taskManager.html",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "taskManager.html"
    },
    "permissions": [
        "tabs"
    ]
}